<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="double_fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item> 
    </style>

    <style name="double_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
      <style name="image_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">60dip</item>
    </style>
       
    <style name="wfill_hwrap">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wwrap_hfill">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <!-- 主界面radiobutton -->
    <style name="tab_frist_work">
        <item name="android:background">@drawable/tab_work</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2.0dp</item>
        <item name="android:button">@null</item>
    </style>

    <style name="tab_second_approve">
        <item name="android:background">@drawable/tab_approve</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2.0dp</item>
        <item name="android:button">@null</item>
    </style>

    <style name="tab_thrid_performance">
        <item name="android:background">@drawable/tab_performance</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">2.0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
    </style>

    <style name="tab_fourth_notice">
        <item name="android:background">@drawable/tab_notice</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">2.0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
    </style>

    <style name="main_tab_bottom">
        <item name="android:textSize">@dimen/bottom_tab_font_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingTop">@dimen/bottom_tab_padding_up</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
   		 <item name="android:layout_weight">1.0</item> 
        <item name="android:drawablePadding">@dimen/bottom_tab_padding_drawable</item> 
	 </style>

    
    <style name="noticeDialog2" parent="@android:style/Theme.Dialog">

        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    
     <style name="Transparent">
    	<item name="android:windowBackground">@color/transparent_background</item>  
    	<item name="android:windowNoTitle">true</item> 
    	<item name="android:windowIsTranslucent">true</item> 
    	<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>
    </style>
    <color name="transparent_background">#50000000</color>    
    
     
     
  <style name="AnimationPreview" parent="android:Animation" mce_bogus="1">        
    </style>   

  
	<style name="MyDialogStyle"> 
			<item name="android:windowBackground">@android:color/transparent</item> 
			<item name="android:windowFrame">@null</item> 
			<item name="android:windowNoTitle">true</item> 
			<item name="android:windowIsFloating">true</item> 
			<item name="android:windowIsTranslucent">true</item> 
			<item name="android:windowContentOverlay">@null</item> 
			<item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item> 
			<item name="android:backgroundDimEnabled">true</item> 
	</style> 
	
	
	 
	<style name="Theme.Start" parent="android:Theme">
        <item name="android:windowBackground">@drawable/loading</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:animationDuration">1500</item>
    </style>
    
</resources>